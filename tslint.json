{
	"extends": ["tslint:all", "tslint-config-prettier"],
	"rules": {
		"completed-docs": {
			"options": {
				"properties": {
					"privacies": "public"
				}
			}
		},
		"file-name-casing": [true, "kebab-case"],
		"no-console": false,
		"no-debugger": false,
		"no-null-keyword": false,
		"prefer-function-over-method": false,
		"typedef": false
	}
}
