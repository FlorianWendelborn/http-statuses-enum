{
	"extends": ["tslint:all", "tslint-config-prettier"],
	"rules": {
		"curly": [true, "as-needed"],
		"completed-docs": {
			"options": {
				"properties": {
					"privacies": "public"
				}
			}
		},
		"file-name-casing": [true, "kebab-case"],
		"no-console": false,
		"no-debugger": false,
		"no-null-keyword": false,
		"max-file-line-count": false,
		"prefer-function-over-method": false,
		"typedef": false
	}
}
